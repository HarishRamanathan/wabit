<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<registry>
    <pack name="Core">
    	<key	root="HKCR"
    			keypath=".wabit"/>
		<value	root="HKCR"
    			name=""
			   	keypath=".wabit"
		       	string="wabit_auto_file"/>
		       	
		<key   	keypath="wabit_auto_file"
    		   	root="HKCR"/>
        <value 	name=""
			   	keypath="wabit_auto_file"
			   	root="HKCR"
		       	string=""/>
		<value 	name="EditFlags"
			   	keypath="wabit_auto_file"
			   	root="HKCR"
		       	string="dword:00000000"/>
		<value 	name="BrowserFlags"
			   	keypath="wabit_auto_file"
			   	root="HKCR"
		       	string="dword:00000008"/>     
		
		<key	keypath="wabit_auto_file\DefaultIcon"
				root="HKCR"/>
		<value 	name=""
			   	keypath="wabit_auto_file\DefaultIcon"
			   	root="HKCR"
		       	string="$INSTALL_PATH\wabit.ico,0"/>
    
    	<key   	keypath="wabit_auto_file\shell\open\command"
    		   	root="HKCR"/>
        <value 	name=""
			   	keypath="wabit_auto_file\shell\open\command"
			   	root="HKCR"
		       	string="javaw -Xms256m -Xmx1024m -jar &quot;$INSTALL_PATH\wabit.jar&quot; &quot;%1&quot; %*"/>
    </pack>
</registry>

